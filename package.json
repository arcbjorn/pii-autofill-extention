{
  "name": "pii-autofill-extension",
  "version": "1.0.0",
  "description": "Chrome extension for PII autofill with intelligent field detection",
  "main": "dev/dev-server.js",
  "scripts": {
    "dev": "node dev/dev-server.js",
    "start": "pnpm run dev",
    "build": "node scripts/build.js",
    "copy-files": "mkdir -p dist && cp -r src/* dist/ && cp manifest.json dist/",
    "clean": "rm -rf dist/*",
    "watch": "pnpm run dev",
    "install": "./scripts/install.sh"
  },
  "keywords": [
    "chrome-extension",
    "autofill",
    "pii",
    "form-detection"
  ],
  "author": "Developer",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "ws": "^8.14.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}